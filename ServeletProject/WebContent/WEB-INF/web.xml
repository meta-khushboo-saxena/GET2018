<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_ID" version="2.4">
  <display-name>addEmployeepage</display-name>
  <servlet>
    <display-name>myservlet</display-name>
    <servlet-name>AddEmployee</servlet-name>
    <servlet-class>com.servlets.AddEmployee</servlet-class>
  </servlet>
  <servlet>
    <display-name>myservlet</display-name>
    <servlet-name>ShowEmployee</servlet-name>
    <servlet-class>com.servlets.ShowEmployee</servlet-class>
  </servlet>
  <servlet>
    <display-name>myservlet</display-name>
    <servlet-name>SearchEmployee</servlet-name>
    <servlet-class>com.servlets.SearchEmployee</servlet-class>
  </servlet>
  <servlet>
    <display-name>myservlet</display-name>
    <servlet-name>UpdateDetails</servlet-name>
    <servlet-class>com.servlets.UpdateDetails</servlet-class>
  </servlet>
  <servlet>
    <display-name>myservlet</display-name>
    <servlet-name>Update</servlet-name>
    <servlet-class>com.servlets.Update</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddEmployee</servlet-name>
    <url-pattern>/AddEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowEmployee</servlet-name>
    <url-pattern>/ShowEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchEmployee</servlet-name>
    <url-pattern>/SearchEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateDetails</servlet-name>
    <url-pattern>/UpdateDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Update</servlet-name>
    <url-pattern>/Update</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>